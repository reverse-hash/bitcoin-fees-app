{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": "./src",
    "checkJs": false,
    "jsx": "react-native",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "paths": {
      "@/assets": ["assets"],
      "@/components": ["common/components"],
      "@/hooks": ["common/hooks"],
      "@/schemas": ["common/schemas"],
      "@/*": ["*"]
    },
    "strictNullChecks": true
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
